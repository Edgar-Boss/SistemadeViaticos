<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SAVIVE.PaginaRechazoSolicitud">
    <ContentPage.Content>
        <StackLayout>
            <Label  Text="Rechazo de solicitud"
                    FontSize="24"
                    FontAttributes="bold"
                    HorizontalOptions="Center"
                    Margin="0,80,0,0"
                    />
            <Label Text="Detalles"
                   Margin="10,130,0,0"/>
            <Frame Margin="15,5" HeightRequest="200">
                <Label Text="Detales del error" 
                       HorizontalTextAlignment="Center"
                       VerticalTextAlignment="Center"/>
            </Frame>
            <StackLayout Orientation="Horizontal" HorizontalOptions="Center">
                <Button Text="Aceptar"  />
                <Button Text="Cancelar" />
            </StackLayout>
            
            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>